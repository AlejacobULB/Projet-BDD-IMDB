R1:

Select distinct a.Nom, a.Prenom, a.Numero From Role a where 5 = (Select count(o.AnneeSortie) From Oeuvre o where o.AnneeSortie >=2003 and o.AnneeSortie <= 2007 and a.OID = o.ID);

R2:
Select distinct a.Nom, a.Prenom, a.Numero From EcritPar a, Oeuvre o where a.OID = o.ID having by count(a.OID) >= 2;

