R4:
	select distinct e.EpisodeID, e.SID From Episode e where no exists( Select a.OID from Role a, Personne p where p.Numero = a.Numero and a.Nom = P.Nom and a.Prenom = p.Prenom and p.Genre = 'm' and a.OID = e.EpisodeID);

